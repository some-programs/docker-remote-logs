{
  "compilerOptions": {
    "incremental": true,
    "target": "es6",
    "module": "none",
    "outDir": "./static/js",
    "rootDir": "./static/ts",
    "strict": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "declaration": false
  }
}
