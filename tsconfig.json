{
  "compilerOptions": {
    "incremental": true,
    "target": "es6",
    "module": "none",
    "outDir": "./assets/static/js",
    "strict": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "declaration": false,
    "tsBuildInfoFile": "./.cache/ts"
  },
  "include": ["ts/**/*"]
}
